{
    "name": "itecommpay/qa-paracestamol",
    "description": "Parallel test runner for Codeception framework",
    "keywords": [
        "codeception",
        "parallel",
        "process"
    ],
    "homepage": "https://github.com/ITECOMMPAY/qa-paracestamol/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Demchenko"
        },
        {
            "name": "Andrey Atavin"
        },
        {
            "name": "Egor Nelyubov"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-xmlreader": "*",
        "codeception/codeception": "^4.1",
        "doctrine/annotations": "^1.10.4",
        "guzzlehttp/guzzle": "^6.0",
        "php-di/php-di": "^6.3",
        "php-ds/php-ds": "^1.3",
        "symfony/process": "^4.0",
        "symfony/property-access": "^4.0",
        "symfony/serializer": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Paracestamol\\": "src",
            "ParacestamolTests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "bin": ["paracestamol"]
}
